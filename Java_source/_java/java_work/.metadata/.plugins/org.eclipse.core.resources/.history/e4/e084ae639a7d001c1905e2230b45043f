package ch00_baekjoon;
import java.util.ArrayList;
import java.util.Scanner;
public class baekjoon11720 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		String num = sc.next();
		int arr[];
		for(int i=0;i<n;i++){
			arr[i] = (int)num.charAt(i);
		}
	}
}
